{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string"
    },
    "@type": {
      "type": "string",
      "enum": ["dcat:Dataset", "http://www.w3.org/ns/dcat#Dataset"]
    },
    "https://w3id.org/cbm/v0.0.1/ns/hasSample": {
      "type": ["string", "array"]
    },
    "https://w3id.org/cbm/v0.0.1/ns/isSampleOf": {
      "type": "null"
    }
  },
  "required": [
    "@id",
    "@type"
  ],
  "not": { "required": [ "id" ] }
}