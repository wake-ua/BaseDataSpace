{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@context": {},
    "@id": {
      "type": "string"
    },
    "@type": {
      "type": "string",
      "enum": ["dcat:Dataset", "http://www.w3.org/ns/dcat#Dataset"]
    },
    "https://w3id.org/cbm/v0.0.1/ns/hasSample": {
      "type": ["string", "array"]
    },
    "https://w3id.org/cbm/v0.0.1/ns/isSampleOf": {
      "type": "null"
    },
    "http://purl.org/dc/terms/title": {
      "type": "string"
    },
    "http://www.w3.org/ns/dcat#keyword": {
      "oneOf": [
        {
          "type": "array",
          "items": [
            {
              "type": "string"
            }
          ]
        },
        {
          "type": "string"
        }
      ]
    },
    "http://purl.org/dc/terms/description": {
      "type": "string"
    },
    "http://purl.org/dc/terms/modified": {
      "type": "string",
      "format": "date-time"
    },
    "http://purl.org/dc/terms/issued": {
      "type": "string",
      "format": "date-time"
    },
    "http://purl.org/dc/terms/accessRights": {
      "type": "string",
      "format": "uri"
    },
    "contenttype": {
      "type": "string"
    },
    "http://www.w3.org/ns/dcat#distribution": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "enum": ["dcat:Distribution", "http://www.w3.org/ns/dcat#Distribution"]
        },
        "name": {
          "type": "string"
        },
        "http://purl.org/dc/terms/title": {
          "type": "string"
        },
        "http://www.w3.org/ns/dcat#accessURL": {
          "type": "string",
          "format": "uri"
        },
        "proxyPath": {
          "type": "string",
          "enum": ["true", "false"]
        },
        "http://purl.org/dc/terms/format": {
          "type": "object",
          "properties": {
            "@id": {
              "type": "string",
              "enum": ["HttpData"]
            }
          }
        },
        "authKey": {
          "type": "string"
        },
        "authCode": {
          "type": "string"
        },
        "https://w3id.org/cbm/v0.0.1/ns/hasDataDictionary": {
          "type": "object",
          "properties": {
            "@type": {
              "type": "string",
              "enum": ["cbm:DataDictionary", "https://w3id.org/cbm/v0.0.1/ns/DataDictionary"]
            },
            "https://w3id.org/cbm/v0.0.1/ns/hasField": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "@type": {
                      "type": "string",
                      "enum": ["cbm:Field", "https://w3id.org/cbm/v0.0.1/ns/Field"]
                    },
                    "https://schema.org/name": {
                      "type": "string"
                    },
                    "http://purl.org/dc/terms/title": {
                      "type": "string"
                    },
                    "http://purl.org/dc/terms/description": {
                      "type": "string"
                    },
                    "https://w3id.org/cbm/v0.0.1/ns/sampleValue": {
                    },
                    "https://w3id.org/cbm/v0.0.1/ns/type": {
                      "type": "string",
                      "enum": ["boolean", "date", "dateTime", "decimal", "double", "float", "integer", "string", "time"]
                    }
                  },
                  "required": [
                    "@type",
                    "https://schema.org/name"
                  ],
                  "additionalProperties": false
                }
              ]
            }
          },
          "required": [
            "@type",
            "https://w3id.org/cbm/v0.0.1/ns/hasField"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "@type",
        "http://www.w3.org/ns/dcat#accessURL",
        "http://purl.org/dc/terms/format"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "@id",
    "@type",
    "http://purl.org/dc/terms/title",
    "http://purl.org/dc/terms/description",
    "http://purl.org/dc/terms/modified",
    "http://purl.org/dc/terms/issued",
    "http://purl.org/dc/terms/accessRights",
    "http://www.w3.org/ns/dcat#distribution"
  ],
  "not": { "required": [ "id" ] },
  "additionalProperties": false
}

