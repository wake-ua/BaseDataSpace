{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string"
    },
    "@type": {
      "type": "string",
      "enum": ["dcat:Dataset", "http://www.w3.org/ns/dcat#Dataset"]
    },
    "https://w3id.org/cbm/v0.0.1/ns/hasSample": {
      "type": ["string", "array"]
    },
    "https://w3id.org/cbm/v0.0.1/ns/isSampleOf": {
      "type": "null"
    },
    "http://purl.org/dc/terms/title": {
      "type": "string"
    },
    "http://purl.org/dc/terms/description": {
      "type": "string"
    },
    "http://purl.org/dc/terms/modified": {
      "type": "string",
      "format": "date-time"
    },
    "http://purl.org/dc/terms/issued": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "@id",
    "@type",
    "http://purl.org/dc/terms/title",
    "http://purl.org/dc/terms/description",
    "http://purl.org/dc/terms/modified",
    "http://purl.org/dc/terms/issued"
  ],
  "not": { "required": [ "id" ] }
}

