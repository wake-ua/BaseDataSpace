

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string"
    },
    "properties": {
      "type": "object",
      "properties": {
        "http://purl.org/dc/terms/title": {
          "type": "string"
        },
        "http://purl.org/dc/terms/description": {
          "type": "string"
        },
        "contenttype": {
          "type": "string"
        },
        "http://purl.org/dc/terms/modified": {
          "type": "string",
          "format": "date-time"
        },
        "http://purl.org/dc/terms/issued": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "http://purl.org/dc/terms/title",
        "http://purl.org/dc/terms/description",
        "contenttype",
        "http://purl.org/dc/terms/issued",
        "http://purl.org/dc/terms/modified"
      ]
    }
  },
  "dataAddress": {
    "type": "object",
    "properties": {
      "type": "string",
      "name": "string",
      "baseUrl": "string",
      "proxyPath": "string"
    }
  },
  "required": [
    "@id",
    "properties",
    "dataAddress"
  ]
}

